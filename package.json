{
  "name": "@lilib/hooks",
  "version": "0.1.0",
  "description": "React hooks library",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "docusaurus start",
    "test": "jest",
    "lint": "tsc -p tsconfig.json --noEmit && eslint . --ext .jsx,.js,.ts,.tsx --fix && prettier --write . --loglevel warn",
    "build": "rimraf dist && concurrently --kill-others-on-fail npm:build:*",
    "build:cjs": "babel src --out-dir dist/cjs --config-file ./configs/babel.cjs.json --extensions .ts,.tsx --source-maps --no-babelrc",
    "build:esm": "babel src --out-dir dist/esm --config-file ./configs/babel.esm.json --extensions .ts,.tsx --source-maps --no-babelrc",
    "build:types": "tsc -p ./configs/tsconfig.types.json",
    "docs:build": "docusaurus build",
    "docs:deploy": "gh-pages -d build",
    "release": "npm run lint && npm run test && npm run build && npm publish && npm run docs:build && npm run docs:deploy"
  },
  "keywords": [
    "react",
    "hooks",
    "library"
  ],
  "author": "李蔚生",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lilibraries/hooks.git"
  },
  "bugs": {
    "url": "https://github.com/lilibraries/hooks/issues"
  },
  "homepage": "https://github.com/lilibraries/hooks#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "@types/js-cookie": "^2.2.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "js-cookie": "^2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@docusaurus/core": "2.0.0-alpha.70",
    "@docusaurus/preset-classic": "2.0.0-alpha.70",
    "@mdx-js/react": "^1.6.22",
    "@sinonjs/fake-timers": "^6.0.1",
    "@testing-library/react-hooks": "^3.7.0",
    "@tweenjs/tween.js": "^18.6.4",
    "@types/jest": "^26.0.19",
    "@types/sinonjs__fake-timers": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-eslint": "^10.1.0",
    "clsx": "^1.1.1",
    "commitizen": "^4.2.2",
    "concurrently": "^5.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "docusaurus-plugin-module-alias": "^0.0.2",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-test-renderer": "^17.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  }
}
